            android:layout_height="match_parent"
            android:layout_weight="640"
            android:orientation="horizontal"
            >
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn_free"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="126"
                android:background="@drawable/selelctor_freetrip"
                />
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn_backpacking"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="126"
                android:background="@drawable/selelctor_backpacking"/>
            <androidx.appcompat.widget.AppCompatButton
