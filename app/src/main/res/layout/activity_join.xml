                android:layout_height="match_parent"
                android:layout_weight="126"
                android:background="@drawable/selelctor_pkg"/>
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn_alone"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="126"
                android:background="@drawable/selelctor_alone"/>
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn_together"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="126"
                android:background="@drawable/selelctor_together"/>
        </LinearLayout>
